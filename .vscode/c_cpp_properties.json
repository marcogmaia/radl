{
    "configurations": [
        {
            "name": "RADL",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib/raylib/src",
                "${workspaceFolder}/lib/raylib/src/external"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/lib/raylib/**"
                ]
            },
            "defines": [
                "SUPPORT_TRACELOG",
                "RLGL_IMPLEMENTATION",
                "SPDLOG_FMT_EXTERNAL",
                "DEBUG"
            ],
            "cStandard": "c99",
            "cppStandard": "c++20"
        }
    ],
    "version": 4
}